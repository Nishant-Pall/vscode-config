// Place your key bindings in this file to override the defaults
[
    // {
    //     "key": "enter",
    //     "command": "macros.openFileAndKeep",
    //     "when": "listFocus"
    // },
    { "key": "ctrl+shift+k", "command": "workbench.action.terminal.focusNext" },
    {
        "key": "ctrl+shift+j",
        "command": "workbench.action.terminal.focusPrevious"
    },
    {
        "key": "shift+alt+]",
        "command": "workbench.panel.defaultLocation"
    },
    {
        "key": "shift+z shift+e",
        "command": "workbench.action.toggleZenMode"
    },
    {
        "key": "ctrl+k z",
        "command": "-workbench.action.toggleZenMode"
    },
    {
        "key": "alt+t",
        "command": "workbench.action.terminal.sendSequence",
        "args": { "text": "cd '${fileDirname}'\u000D" }
    },
    {
        "key": "shift+alt+n",
        "command": "terminalHere.create"
    },
    {
        "key": "alt+n",
        "command": "workbench.action.positionPanelBottom"
    },
    {
        "key": "alt+m",
        "command": "workbench.action.positionPanelRight"
    },
    {
        "key": "shift+end",
        "command": "cursorEnd",
        "when": "textInputFocus"
    },
    {
        "key": "end",
        "command": "-cursorEnd",
        "when": "textInputFocus"
    },
    {
        "key": "shift+end",
        "command": "-cursorEndSelect",
        "when": "textInputFocus"
    },
    {
        "key": "shift+numpad7",
        "command": "cursorHomeSelect",
        "when": "textInputFocus"
    },
    {
        "key": "shift+home",
        "command": "-cursorHomeSelect",
        "when": "textInputFocus"
    },
